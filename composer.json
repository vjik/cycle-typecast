{
    "name": "vjik/cycle-columns",
    "type": "library",
    "description": "Helper of modify data when mapping in Cycle ORM",
    "keywords": [
        "cycle",
        "datamapper"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/vjik/cycle-columns/issues?state=open",
        "source": "https://github.com/vjik/cycle-columns"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.6",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Vjik\\CycleColumns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\CycleColumns\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    }
}
